datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  models   = [User]
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String   @id @default(uuid())
  name       String
  password   String
  email      String   @unique
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
